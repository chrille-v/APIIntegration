{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Api": {
    "BaseUrl": "https://api.example.com",
    "ApiKey": "123"
  },
  "Polling": {
    "IntervalSeconds": 30,
    "RetryBackoffSeconds": 5,
    "MaxBackoffSeconds": 300
  },
  "Database": {
    "Path": "localMessages.db"
  },
  "Offline": {
    "CheckIntervalSeconds": 10
  },
  "ConnectionStrings": {
    "LocalDB": "Server=(LocalDB)\\MSSQLLocalDB;Database=LocalDB;Integrated Security=True;"
  },
  "CloudWorker": {
    "BatchSize": 20,
    "PollIntervals": 3000
  }
}
